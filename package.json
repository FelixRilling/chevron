{
    "name": "chevronjs",
    "version": "v4.2.0",
    "description": "Chevron is an extremely small(1.1kB) JavaScript service library for easy dependency managment and lazy module loading.",
    "main": "dist/chevron.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/chevronjs.git"
    },
    "scripts": {
        "start": "npm run bundle && npm run babel && npm run uglify",
        "bundle": "rollup -c",
        "babel": "babel ./dist/es6 --out-dir ./dist",
        "uglify": "uglifyjs ./dist/chevron.js -o ./dist/chevron.min.js --screw-ie8 --mangle --compress conditionals"
    },
    "keywords": [
        "dependencies",
        "services"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/chevronjs/issues"
    },
    "homepage": "https://github.com/FelixRilling/chevronjs#readme",
    "devDependencies": {
        "babel-core": "^6.0.20",
        "babel-preset-es2015": "^6.13.0",
        "rollup": "^0.34.1",
        "rollup-plugin-commonjs": "^3.3.1",
        "rollup-plugin-node-resolve": "^2.0.0",
        "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}
