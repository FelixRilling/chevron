{
    "name": "chevronjs",
    "version": "9.3.0",
    "description": "A super tiny JavaScript library for module declaration",
    "main": "dist/chevron.common.js",
    "module": "dist/chevron.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "Chevron",
                "file": "chevron"
            },
            "targets": {
                "browsers": ["Chrome >= 60"]
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/chevronjs.git"
    },
    "keywords": ["dependencies", "modules", "service", "factory"],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/chevronjs/issues"
    },
    "homepage": "https://github.com/FelixRilling/chevronjs#readme",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.13.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "jasmine": "^2.8.0",
        "npm-run-all": "^4.1.1",
        "rollup": "^0.52.1",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "uglify-es": "^3.2.1"
    }
}
