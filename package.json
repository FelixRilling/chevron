{
    "name": "chevronjs",
    "version": "v5.2.0",
    "description": "Chevron is an extremely small(1.0kB) JavaScript service library for easy dependency managment and lazy module loading.",
    "main": "dist/chevron.js",
    "module": {
        "name": "Chevron",
        "id": "chevron"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/chevronjs.git"
    },
    "scripts": {
        "start": "npm run sync  && npm run bundle && npm run babel && npm run uglify && npm run head",
        "sync": "node ./tasks/sync.js",
        "bundle": "rollup -c",
        "babel": "rollup -c ./rollup.babel.config.js",
        "head": "node ./tasks/head.js",
        "uglify": "node ./tasks/uglify.js"
    },
    "keywords": [
        "dependencies",
        "services"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/chevronjs/issues"
    },
    "homepage": "https://github.com/FelixRilling/chevronjs#readme",
    "devDependencies": {
      "babel-plugin-external-helpers": "^6.8.0",
      "babel-preset-es2015": "^6.14.0",
      "rollup": "^0.34.0",
      "rollup-plugin-babel": "^2.6.1",
      "rollup-plugin-commonjs": "^3.3.0",
      "rollup-plugin-node-resolve": "^2.0.0",
      "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}
