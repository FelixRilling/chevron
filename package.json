{
    "name": "chevronjs",
    "version": "8.1.1",
    "description": "A super tiny JavaScript library for module declaration",
    "main": "dist/chevron.common.js",
    "namespace": {
        "module": "Chevron",
        "file": "chevron"
    },
    "scripts": {
        "start": "npm run lint && npm run build && npm run test",
        "build": "node build.js",
        "lint": "./node_modules/.bin/eslint ./src/",
        "test": "./node_modules/.bin/jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/chevronjs.git"
    },
    "keywords": [
        "dependencies",
        "modules",
        "service",
        "factory"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/chevronjs/issues"
    },
    "homepage": "https://github.com/FelixRilling/chevronjs#readme",

    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-es2016": "^6.24.1",
        "eslint": "^3.19.0",
        "jasmine": "^2.5.3",
        "rollup": "^0.41.6",
        "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony"
    },
    "dependencies": {}
}
